{
  "name": "Container Default Pages",
  "version": "0.0.1",
  "manifest_version": 2,
  "incognito": "not_allowed",
  "description": "Specify a default page for each container.",
  "homepage_url": "https://github.com/Allabbad/FF-Container-Default-Pages",
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  },
  "permissions": [
    "activeTab",
    "contextualIdentities",
    "storage"
  ],
  "background": {
    "page": "js/background/index.html"
  },
  "applications": {
    "gecko": {
      "id": "@testpilot-container-default-pages",
      "strict_min_version": "67.0"
    }
  }
}
